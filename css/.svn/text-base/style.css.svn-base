: 87%;}#buckets p.small{font-size: 12px;}.slides_container {	width:320px;	height:240px;	overflow:hidden;	position:relative;	display:none;	 -webkit-box-shadow:  0px 3px 18px 0px #aaa;        -moz-box-shadow:  0px 3px 18px 0px #aaa;        box-shadow:  0px 3px 18px 0px #aaa;	z-index: 2;	background: #fff;	        }.small_slides .slide {	padding:10px 20px 0;	padding-bottom: 0;	width:300px;	height:240px;	display:block;	background: #fff;	}.slide_title{	font-size: 18px;	color: #000;	margin-top: 15px;}.slide_subtitle{	font-size: 17px;	color: #000;	margin-top: 8px;	text-transform:capitalize;	margin-bottom: 4px;}.left_content .slide_subtitle{	font-size: 18px;	margin-bottom: 4px;}.small_slides .right_content{	width: 46%;}.small_slides .left_content{	width: 54%; }.small_slides .left_content2{	width: 37%;}.small_slides .right_content2{	width: 58%; 	margin-top: 39px;	margin-right: 13px;}.caricatures{    position: absolute;    top: 60px;    right:  20px;    z-index: -1;}/*-------Next/prev buttons-----*/.small_slides a.next{	width:17px;	height:18px;	display:block;	z-index:101;	background:url(../img/front/arrow_next_small.png) no-repeat;	position: relative;	top: -27px;	left: 23px;}.small_slides a.next:hover{	background:url(../img/front/arrow_next_small_hover.png) no-repeat;}/*----------Pagination---------*/.pagination {	position: relative;	top: -20px; 	left: 20px; 	z-index:101;}.pagination li {float:left; margin:0 1px; list-style:none;}.pagination li a {	display:block;	width:6px;	height:5px;	background-image:url(../img/front/pagination_small.png);	background-repeat: no-repeat;	background-position:0 0;	float:left;	overflow:hidden;	text-indent: -99999%;}.pagination li.current a {	background-position:0 -7px;}/*----------------------------------------------*/.small_slides .link {	display:block;	margin-top:10px;	font-weight:800;}.clock {		float:left; 		display:block;		margin:9px 15px 9px 9px; 		background-color:#fff;		text-align: center;			}.number_sign{	display: inline-block;	position: relative;	top: -58px;}.big{	font-family: 'BitterBold';	font-size: 54px;	}.bigger{	display: inline-block;	position: relative;	top: -15px;	font-family: 'BitterBold';	font-size: 110px;}.tilted{	width: 1040px;	height: 175px;	background: url(../img/front/stripes.png);	position: absolute;	top: 87px;	left: -22px;	z-index:  1;}/* FlexSlider Necessary Styles*********************************/ .flexslider {width: 100%; margin: 0; padding: 0; height: 557px;}.flexslider .slides li {display: none;} /* Hide the slides before the JS is loaded. Avoids image jumping */.flexslider .slides img {max-width: 100%; display: block;}/* FlexSlider Default Theme*********************************/.flexslider {position: relative; -webkit-border-radius: 5px; -moz-border-radius: 5px; -o-border-radius: 5px; border-radius: 5px;}.flexslider .slides  li {position: relative; top: 0px;}.flexslider .slides  li.inner { top: 16px;}/* Suggested container for "Slide" animation setups. Can replace this with your own, if you wish */.flexslider-container {position: relative; width: 1100px; margin: 0 auto; left: 50px; top: -14px;}/* Caption style */.tagline{width: 420px; position: absolute; top: 100px; }.tagline h1{font-size: 36px; line-height: 36px; margin-bottom: 15px; color: #9d9c99;}.tagline p{ width: 335px;}.call_to_action{width: 207px; padding: 8px 0; text-align: center;  display: block; background:#ff5900;  font-size: 13px;  font-weight: bold; color: #fff; margin: 20px 0 0 0px; text-shadow:none; letter-spacing: -0.07em; text-transform:uppercase;}a.call_to_action:hover{	-webkit-box-shadow: inset 0px 0px 15px -3px #852b00;	 box-shadow: inset 0px 0px 17px -3px #852b00;	}.flex-caption {width: 96%; padding: 2%; position: absolute; left: 0; bottom: 0; background: rgba(0,0,0,.3); color: #fff; text-shadow: 0 -1px 0 rgba(0,0,0,.3); font-size: 15px; line-height: 18px;}/* Direction Nav */.flex-direction-nav li a {width: 27px; height: 27px; display: inline-block; background: url(../img/front/arrow_next.png) no-repeat 0 0; position: relative;top: -159px; cursor: pointer; text-indent: -9999px;} .flex-direction-nav li a:hover {background: url(../img/front/arrow_next_hover.png) no-repeat 0 0; }.flex-direction-nav li a.next {left: 25px;}/* Control Nav */.flex-control-nav {position: relative; top: -150px;  left: 20px;} /* set negative margin-left equal to half the width on the control-nav for perfect centering */.flex-control-nav li {margin: 0 0 0 4px; float: left;}.flex-control-nav li:first-child {margin: 0;}.flex-control-nav li a {width: 8px; height: 7px; display: block; background: url(../img/front/pagination.png) no-repeat 0 0; cursor: pointer; text-indent: -9999px;}.flex-control-nav li a:hover {//background-position: 0 -7px;}.flex-control-nav li a.active {background-position: 0 -9px; cursor: default;}/*------------------------------------Work Page-----------------------------------*/.da-thumbs {	list-style: none;	width: 960px;	height: 600px;	position: relative;	margin: 20px auto;	padding: 0;}.da-thumbs li {	height: 168px;	float: left;	margin: 0  8px 30px 0;	position: relative;	}.da-thumbs li.last{margin-right: 0;}.da-thumbs li span{display:block; padding-bottom: 3px; border-bottom: 1px solid #999;}.da-thumbs li:hover span.border{border-bottom: 3px solid #ff5900;}.da-thumbs li p{	margin-top: 10px; 	font:13px 'BitterRegular', Arial, sans-serif; color: #000;}.da-thumbs li:hover p{margin-top: 8px;}	.da-thumbs li a,.da-thumbs li a img {	display: block;	position: relative;}.da-thumbs li a {	overflow: hidden;}.da-thumbs li a div {	position: absolute;	background: rgba(255,89,0,0.9);	width: 100%;	height: 100%;}.da-thumbs li a div span {	display: block;	padding: 20px 0 10px;	margin: 30px 20px 20px 20px;	text-transform: uppercase;	font-weight: normal;	color: rgba(255,255,255,0.9);	text-shadow: 1px 1px 1px rgba(0,0,0,0.2);	border-bottom: 1px dotted rgba(255,255,255,0.5);	background: url(../img/work/tilted2.png) repeat-x left 0%;}.da-thumbs li a div.da-animate {	-webkit-transition: all 0.3s ease;	-moz-transition: all 0.3s ease-in-out;	-o-transition: all 0.3s ease-in-out;	-ms-transition: all 0.3s ease-in-out;	transition: all 0.3s ease-in-out;}/* Initial state classes: */.da-slideFromTop {	left: 0px; top: -100%;}.da-slideFromBottom {left: 0px; 	top: 100%;}.da-slideFromLeft {top: 0px;  left: -100%;}.da-slideFromRight {top: 0px; left: 100%;}/* Final state classes: */.da-slideTop {top: 0px; }.da-slideLeft {left: 0px;}/*--------------------Work Details-----------------------*/#content{	width: 797px;	float: right;}#sidebar{	width: 140px;	height: 400px; float: left;}#sidebar h2{font-size: 24px; color: #000; line-height: 28px;}/* Anything slider*/#slider2 { width: 800px; height: 442px; list-style: none; }/* Set slider2 panel sizes, Main & FX demo pages */#slider2 .panel1 { width: 800px; height: 451px}#slider2 .panel2 { width: 800px; height: 102px}#slider2 .panel3 { width: 800px; height: 360px}#slider2 .panel4 { width: 800px; height: 452px} /* With no specific size, it defaults to wrapper size; except in IE7, it needs a width defined, so set to 100% */#slider2 .panel5 { width: 800px; height: 442px}#slider2 .panel6 { width: 800px; height: 442px}#slider2 .panel7 { width: 800px; height: 360px}#slider2 .panel8 { width: 800px; height: 442px} /* With no specific size, it defaults to wrapper size; except in IE7, it needs a width defined, so set to 100% */#slider2 .panel9 { width: 800px; height: 442px}#slider2 .panel10 { width: 800px; height: 442px}/* Active State (has keyboard focus)*/.thumbNav li{float: left;margin: 10px 18px 10px 0px;padding-bottom: 4px;}.thumbNav li:nth-child(5n+0){margin-right: 0;}.thumbNav li.cur{border-bottom: 1px solid #ff5900;	margin-bottom: 5px;}.thumbNav a {display: block;}.thumbNav li a,.thumbNav li a img {	display: block;	position: relative;}.thumbNav li a {	overflow: hidden;}.thumbNav li a div {	position: absolute;	background: rgba(255,89,0,0.9);	width: 100%;	height: 100%;}.thumbNav li a div span {	display: block;	padding: 16px 0px 5px;	margin: 12px 5px 0;	text-transform: uppercase;	text-align: center;	font-weight: normal;	color: rgba(255,255,255,0.9);	text-shadow: 1px 1px 1px rgba(0,0,0,0.2);	border-bottom: 1px dotted rgba(255,255,255,0.5);	background: url(../img/work/tilted2.png) repeat-x left 0%;	font-size: 10px;}.thumbNav li a div.da-animate {	-webkit-transition: all 0.3s ease;	-moz-transition: all 0.3s ease-in-out;	-o-transition: all 0.3s ease-in-out;	-ms-transition: all 0.3s ease-in-out;	transition: all 0.3s ease-in-out;}/* Initial state classes: */.da-slideFromTop {	left: 0px;top: -100%;}.da-slideFromBottom {left: 0px;top: 100%;}.da-slideFromLeft {top: 0px; left: -100%;}.da-slideFromRight {top: 0px;	left: 100%;}/* Final state classes: */.da-slideTop {top: 0px;}.da-slideLeft {left: 0px;}.anythingControls a {	display: block; width: 145px; height: 82px; } /* NAVIGATION POSITIONING*//* Navigation Arrows */.anythingSlider .arrow {	display: block;	position:absolute;	top: 0;	float: left;	z-index: 3;}.anythingSlider .arrow a {	display: block;	width: 28px;	height: 34px;	text-align: center;	outline: 0;	font-size: 0;	background:url(../img/work_details/motion_real/slider_control.jpg)}/* back arrow */.anythingSlider .back { right: 26px;}.anythingSlider .back a { background-position: 0 -4px; }.anythingSlider .back a:hover { background-position: 0 -35px; }/* forward arrow */.anythingSlider .forward { right: -1px; }.anythingSlider .forward a {background-position: 28px -4px; }.anythingSlider .forward a:hover{background-position: 28px -35px; }/* navigationSize window */.anythingControls .anythingNavWindow {overflow: hidden;float: left;}/*  COMMON SLIDER STYLING*//* Overall Wrapper */.anythingSlider {	display: block;	overflow: visible !important;	position: relative;}/* anythingSlider viewport window */.anythingSlider .anythingWindow {	overflow: hidden;	position: relative;	width: 100%;	height: 100%;}/* anythingSlider base (original element) */.anythingSlider .anythingBase {	background: transparent;	list-style: none;	position: absolute;	overflow: visible !important;	top: 0;	left: 0;	margin: 0;	padding: 0;}/* Navigation arrow text; indent moved to span inside "a", for IE7;  apparently, a negative text-indent on an "a" link moves the link as well as the text */.anythingSlider .arrow span {	display: block;	visibility: hidden;}/* all panels inside the slider; horizontal mode */.anythingSlider .panel {	background: transparent;	display: block;	overflow: hidden;	float: left;	padding: 0;	margin: 0;}/* fade mode */.anythingSlider .fade .panel {	float: none;	opacity: 0;	filter: alpha(opacity=0);	position: absolute;	top: 0;	left: 0;}/* fade mode active page - visible & on top */.anythingSlider .fade .activePage {	opacity: 1;	filter: alpha(opacity=100);	z-index:2;}/*social icons*/.social{font-size: 0; margin-top: 20px;}.social li{	float: left; margin-right: 5px;}.social li a{display: block; width: 16px; height: 16px; background: url(../img/social.png) no-repeat;}.social li a.facebook{background-position: 0 0;}.social li a.google-plus{background-position: -18px 0;}.social li a.linkedin{background-position:  -36px 0;}.social li a.twitter{background-position: -54px 0;}.social li a.facebook:hover{background-position: 0 -18px;}.social li a.google-plus:hover{background-position: -18px -18px;}.social li a.linkedin:hover{background-position:  -36px  -18px;}.social li a.twitter:hover{background-position: -54px -18px;}	 	/*---------------------Content-----------------------*/.filter-cont{ position: relative; padding-bottom: 15px; margin: 25px 0 15px;  border-bottom: 1px s